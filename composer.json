{
    "name": "andrey-mashukov/networkstat",
    "keywords": ["networkstats", "hashrate", "transactions", "marketcap"],
    "homepage": "https://github.com/AndreyMashukov/networkstat",
    "description": "Network statistics and information",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Mashukov",
            "email": "a.mashukov@yandex.ru",
            "role": "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "AM\\Networkstat\\": ["src"]
        },
        "classmap": [
            "src"
        ]
    },
    "require": {
        "php": ">=7",
        "logics/http": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\": ["tests"]
        }
    },
    "require-dev": {
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^5.2",
        "logics/phpunit-extensions": "^0.2||dev-trunk",
        "logics/codesniffer": "^0.1||dev-trunk",
        "logics/extcheck": "^0.1||dev-trunk"
    },
    "prefer-stable": true
}